<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<TitledPane fx:id="titledPane" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.jackalope.thumptest.controller.HomeController">
      <ScrollPane fx:id="scrollPane" minHeight="-Infinity" minWidth="-Infinity" prefHeight="470.0" prefWidth="934.0">
        <content>
          <AnchorPane>
            <children>
               <TextArea fx:id="logArea" editable="false" layoutX="14.0" layoutY="14.0" prefHeight="360.0" prefWidth="911.0" />
               <Button fx:id="cpuRunBurnTestButton" layoutX="14.0" layoutY="381.0" mnemonicParsing="false" onAction="#onCpuRunBurnTestButtonClick" text="%button.runCPUBurnTest" />
               <Button fx:id="stopTestsButton" layoutX="744.0" layoutY="381.0" mnemonicParsing="false" onAction="#onStopTestsButtonClick" text="%button.stopTests" />
               <Button fx:id="clearButton" layoutX="865.0" layoutY="381.0" mnemonicParsing="false" onAction="#onClearButtonClick" text="%button.clear" />
               <Button id="cpuRunBenchTestButton" fx:id="cpuRunBenchTestButton" layoutX="175.0" layoutY="381.0" mnemonicParsing="false" onAction="#onCpuRunBenchTestButtonClick" text="%button.runCPUBenchTest" />
            </children></AnchorPane>
        </content>
      </ScrollPane>
   <graphic>
      <MenuBar fx:id="menuBar">
        <menus>
          <Menu mnemonicParsing="false" text="%menuItem.file">
            <items>
              <MenuItem mnemonicParsing="false" text="%menuItem.close" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="%menuItem.edit">
            <items>
              <MenuItem mnemonicParsing="false" text="%menuItem.delete" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="%menuItem.help">
            <items>
              <MenuItem mnemonicParsing="false" text="%menuItem.about" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </graphic>
</TitledPane>
